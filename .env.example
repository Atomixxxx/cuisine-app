# Supabase shared database (required for multi-device sync)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
VITE_SUPABASE_WORKSPACE_ID=default
VITE_SUPABASE_STORAGE_BUCKET=cuisine-media
